{
  "name": "dollarstreet",
  "version": "0.0.1",
  "private": true,
  "description": "consumer.dollarstreet.org angular2 client application",
  "main": "index.js",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --aot --prod",
    "test": "ng test -sr -cc",
    "test:w": "ng test",
    "test:coverage": "ng test --code-coverage",
    "postinstall": "concurrent \"npm run update-webdriver\"",
    "update-webdriver": "webdriver-manager update",
    "deploy:github": "npm run build && npm run publish:gh-pages",
    "deploy:firebase": "npm run build && npm run publish:firebase",
    "publish:gh-pages": "./tools/gh-pages-publish.js",
    "publish:firebase": "firebase deploy",
    "codecov": "cat ./coverage/report-lcov/lcov.info | codecov -t $CODECOV_KEY",
    "pre-commit-test": "karma start ./.config/karma-pre.config.js",
    "eslint": "eslint --ignore-path .gitignore --ignore-path .eslintignore --ext js --fix . .config",
    "lint": "npm run tslint && npm run eslint",
    "tslint": "gulp tslint",
    "e2e:local": "protractor ./.config/protractor.conf.js",
    "e2e:bstack:tunnel": ".config/tools/bstack-local.js",
    "e2e:bstack": ".config/tools/wait4tunnel.js && protractor ./.config/protractor-browserstack.conf.js"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@angular/cli": "1.0.0",
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/compiler-cli": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/router": "4.0.0",
    "@types/d3": "4.4.0",
    "@types/d3-axis": "1.0.7",
    "@types/d3-scale": "1.0.7",
    "@types/d3-selection": "1.0.13",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.44",
    "@types/node": "6.0.54",
    "@types/selenium-webdriver": "2.53.38",
    "angular2-infinite-scroll": "0.2.8",
    "angulartics2": "1.6.1",
    "bootstrap": "3.3.7",
    "d3-axis": "1.0.6",
    "d3-scale": "1.0.5",
    "d3-selection": "1.0.5",
    "d3-interpolate": "1.1.4",
    "es6-promise": "4.0.5",
    "es6-shim": "0.35.2",
    "es7-reflect-metadata": "1.6.0",
    "lodash": "4.*",
    "ng2-bootstrap": "1.1.16",
    "ng2-translate": "5.0.0",
    "rxjs": "5.0.2",
    "typings": "2.1.0",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "babel-polyfill": "6.20.0",
    "browserstack-wrapper": "1.0.0",
    "browserstacktunnel-wrapper": "2.0.0",
    "codecov": "1.0.1",
    "codelyzer": "3.0.0-beta.4",
    "compression": "1.6.2",
    "concurrently": "3.1.0",
    "coveralls": "2.11.15",
    "cpy": "5.0.0",
    "del": "2.2.2",
    "eslint-config-valorsoft": "0.1.0",
    "exports-loader": "0.6.3",
    "file-loader": "0.9.0",
    "firebase": "3.6.4",
    "gh-pages": "0.12.0",
    "gulp": "3.9.1",
    "gulp-tslint": "7.1.0",
    "html-loader": "0.4.4",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-browserstack-launcher": "1.1.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.4.0",
    "karma-sauce-launcher": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.14",
    "pre-commit": "1.2.2",
    "protractor": "5.1.0",
