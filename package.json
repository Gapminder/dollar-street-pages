{
  "name": "dollarstreet",
  "version": "0.0.1",
  "private": true,
  "description": "consumer.dollarstreet.org angular2 client application",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:dev",
    "dev": "NODE_ENV=development webpack-dev-server --inline --hot --progress --color",
    "postinstall": "./node_modules/.bin/concurrent \"npm run update-webdriver\"",
    "update-webdriver": "./node_modules/.bin/webdriver-manager update",
    "deploy:github": "npm run build:prod && npm run publish:gh-pages",
    "deploy:firebase": "npm run build:prod && npm run publish:firebase",
    "publish:gh-pages": "./tools/gh-pages-publish.js",
    "publish:firebase": "firebase deploy",
    "build:dev": "./node_modules/.bin/webpack --progress --color",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress --color",
    "watch:dev": "./node_modules/.bin/webpack --progress --color -w",
    "watch:prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress --color -w",
    "unit-tests": "./node_modules/.bin/karma start -w",
    "codecov": "cat ./coverage/report-lcov/lcov.info | ./node_modules/.bin/codecov -t $CODECOV_KEY",
    "pre-commit-test": "./node_modules/.bin/karma start ./.config/karma-pre.config.js",
    "eslint": "./node_modules/.bin/eslint --ignore-path .gitignore --ignore-path .eslintignore --ext js --fix . .config",
    "lint": "npm run tslint && npm run eslint",
    "tslint": "./node_modules/.bin/gulp tslint",
    "e2e:local": "./node_modules/.bin/protractor ./.config/protractor.conf.js",
    "e2e:bstack:tunnel": ".config/tools/bstack-local.js",
    "e2e:bstack": ".config/tools/wait4tunnel.js && ./node_modules/.bin/protractor ./.config/protractor-browserstack.conf.js"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@angular/cli": "1.0.0-beta.32.3",
    "@angular/common": "2.4.7",
    "@angular/compiler": "2.4.7",
    "@angular/core": "2.4.7",
    "@angular/forms": "2.4.7",
    "@angular/http": "2.4.7",
    "@angular/platform-browser": "2.4.7",
    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/router": "3.4.1",
    "@types/d3": "4.4.0",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.44",
    "@types/node": "6.0.54",
    "@types/selenium-webdriver": "2.53.38",
    "angular2-infinite-scroll": "0.2.8",
    "angulartics2": "1.6.1",
    "bootstrap": "3.3.7",
    "codelyzer": "2.1.1",
    "d3-axis": "1.0.4",
    "d3-scale": "1.0.4",
    "d3-selection": "1.0.3",
    "es6-promise": "4.0.5",
    "es6-shim": "0.35.2",
    "es7-reflect-metadata": "1.6.0",
    "lodash": "4.*",
    "ng2-bootstrap": "1.1.16",
    "ng2-translate": "5.0.0",
    "rxjs": "5.0.2",
    "typings": "2.1.0",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "babel-polyfill": "6.20.0",
    "browserstack-wrapper": "1.0.0",
    "browserstacktunnel-wrapper": "2.0.0",
    "codecov": "1.0.1",
    "compression": "1.6.2",
    "concurrently": "3.1.0",
    "coveralls": "2.11.15",
    "cpy": "5.0.0",
    "del": "2.2.2",
    "eslint-config-valorsoft": "0.1.0",
    "exports-loader": "0.6.3",
    "file-loader": "0.9.0",
    "firebase": "3.6.4",
    "gh-pages": "0.12.0",
    "gulp": "3.9.1",
    "gulp-tslint": "8.0.0",
    "html-loader": "0.4.4",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-browserstack-launcher": "1.1.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.4.0",
    "karma-sauce-launcher": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.14",
    "pre-commit": "1.2.2",
    "protractor": "5.1.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.9",
    "shelljs": "0.7.5",
    "source-map-loader": "0.1.5",
    "ts-loader": "1.3.3",
    "ts-node": "1.7.3",
    "tslint": "4.5.1",
    "tslint-config-valorsoft": "1.1.1",
    "typescript": "2.2.2",
    "url-loader": "0.5.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gapminder/dollar-street-pages.git"
  },
  "keywords": [
    "dollarstreet"
  ],
  "author": "Valor Software",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Gapminder/dollar-street-pages/issues"
  },
  "homepage": "https://github.com/Gapminder/dollar-street-pages#readme"
}
